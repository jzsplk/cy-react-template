{
  "compilerOptions": {
    "baseUrl": "src",
    "rootDirs": [
      "src",
      "cypress"
    ],
    "outDir": "./built",
    "target": "es5",
    "jsx": "preserve",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "allowJs": true
  },
  "include": [
    "src"
  ],
  "files": [
    "src/slack/index.ts"
  ]
}
